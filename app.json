{
  "expo": {
    "name": "HÃ¡bitos",
    "slug": "habits-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "habitsapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/images/app/splash-icon.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.juanprato.habitsapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.juanprato.habitsapp"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/app/splash-icon.png",
          "imageWidth": 200
        }
      ],
      "expo-font",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3a0a436f-f21a-4431-abb5-925484e1c2d0"
      }
    }
  }
}
