{
  "expo": {
    "name": "habits-app",
    "slug": "habits-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "habitsapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.juanprato.habitsapp"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.juanprato.habitsapp"
    },
    "web": {
      "output": "server"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
